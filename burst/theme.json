{
	"settings": {
		"appearanceTools": true,
		"layout": {
			"contentSize": "768px",
			"wideSize": "1024px"
		}
	},
	"styles": {
		"blocks": {
			"core/site-title": {
				"color": {
					"text": "var(--wp--preset--color--black)"
				},
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--black)"
						}
					}
				},
				"typography": {
					"fontSize": "24px",
					"fontStyle": "normal",
					"fontWeight": "600"
				}
			}
		},
		"spacing": {
			"padding": {
				"bottom": "var(--wp--preset--spacing--50)",
				"left": "var(--wp--preset--spacing--50)",
				"right": "var(--wp--preset--spacing--50)",
				"top": "var(--wp--preset--spacing--50)"
			}
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header",
			"title": "Header"
		},
		{
			"area": "footer",
			"name": "footer",
			"title": "Footer"
		}
	],
	"version": 3,
	"$schema": "https://schemas.wp.org/wp/6.8/theme.json"
}