					"fontStyle": "normal",
					"fontWeight": "500",
					"lineHeight": "1.2"
				}
			},
			"h6": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--roboto)",
					"fontSize": "16px",
					"fontStyle": "italic",
					"fontWeight": "500",
					"lineHeight": "1.2"
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--roboto)",
					"fontStyle": "normal",
					"fontWeight": "300"
				}
			},
			"link": {
				":hover": {
					"color": {
						"text": "#eb4f0c"
					}
				},
				"color": {
					"text": "#ebad12"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--roboto)",
					"fontStyle": "normal",
					"fontWeight": "500"
				}
			}
		},
		"spacing": {
			"blockGap": "24px",
			"padding": {
				"bottom": "0px",
				"left": "0px",
				"right": "0px",
				"top": "0px"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--roboto)",
			"fontSize": "var(--wp--preset--font-size--medium)",
			"fontStyle": "normal",
			"fontWeight": "300",
			"lineHeight": "1.7"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header",
			"title": "Header"
		},
		{
			"area": "footer",
			"name": "footer",
			"title": "Footer"
		}
	],
	"version": 3,
	"$schema": "https://schemas.wp.org/wp/6.8/theme.json"
}