{
	"settings": {
		"appearanceTools": true,
		"color": {
			"defaultPalette": false,
			"palette": [
				{
					"color": "#000",
					"name": "Black",
					"slug": "custom-black"
				},
				{
					"color": "#ffffff",
					"name": "White",
					"slug": "custom-white"
				}
			]
		},
		"layout": {
			"contentSize": "768px",
			"wideSize": "1024px"
		},
		"typography": {
			"defaultFontSizes": false,
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontFamily": "Roboto",
							"fontStyle": "normal",
							"fontWeight": "300",
							"src": [
								"file:./assets/fonts/roboto/roboto-300-normal.woff2"
							]
						},
						{
							"fontFamily": "Roboto",
							"fontStyle": "normal",
							"fontWeight": "500",
							"src": [
								"file:./assets/fonts/roboto/roboto-500-normal.woff2"
							]
						},
						{
							"fontFamily": "Roboto",
							"fontStyle": "italic",
							"fontWeight": "300",
							"src": [
								"file:./assets/fonts/roboto/roboto-300-italic.woff2"
							]
						},
						{
							"fontFamily": "Roboto",
							"fontStyle": "italic",
							"fontWeight": "500",
							"src": [
								"file:./assets/fonts/roboto/roboto-500-italic.woff2"
							]
						}
					],
					"fontFamily": "Roboto, sans-serif",
					"name": "Roboto",
					"slug": "roboto"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Splash",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/splash/splash-400-normal.woff2"
							]
						}
					],
					"fontFamily": "Splash, cursive",
					"name": "Splash",
					"slug": "splash"
				}
			],
			"fontSizes": [
				{
					"fluid": true,
					"name": "Small",
					"size": "14px",
					"slug": "small"
				},
				{
					"fluid": true,
					"name": "Medium",
					"size": "18px",
					"slug": "medium"
				},
				{
					"fluid": true,
					"name": "Large",
					"size": "24px",
					"slug": "large"
				},
				{
					"fluid": true,
					"name": "Extra Large",
					"size": "32px",
					"slug": "x-large"
				}
			]
		}
	},
	"styles": {
		"color": {
			"background": "#191919",
			"text": "var(--wp--preset--color--custom-white)"
		},
		"elements": {
			"h1": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--roboto)",
					"fontSize": "32px",
					"fontStyle": "normal",
					"fontWeight": "500",
					"lineHeight": "1.2"
				}
			},
			"h2": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--roboto)",
					"fontSize": "24px",
					"fontStyle": "normal",
					"fontWeight": "500",
					"lineHeight": "1.2"
				}
			},
			"h3": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--roboto)",
					"fontSize": "20px",
					"fontStyle": "normal",
					"fontWeight": "500",
					"lineHeight": "1.2"
				}
			},
			"h4": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--roboto)",
